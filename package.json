    "@influxdata/influxdb-client": "^1.11.0",
    "cors": "^2.8.5",
    "env-var": "^7.0.0",
    "express": "^4.17.1",
    "express-openapi": "^7.5.0",
    "jsonwebtoken": "^8.5.1",
    "keycloak-connect": "^12.0.4",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.1",
    "query-string": "^6.14.1"
  },
  "devDependencies": {
    "depcheck": "^1.4.0",
    "forever": "^3.0.4",
    "prettier": "^2.2.1",
