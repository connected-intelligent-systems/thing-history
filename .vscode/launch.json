    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Program",
            "env": {
                "PORT": "9221",
                "RESOURCE_SERVER_AUDIENCE": "resources",
                "KEYCLOAK_HOST": "http://keycloak:8080",
                "KEYCLOAK_REALM": "foresight"
            },
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/index.js"
        }
    ]
}